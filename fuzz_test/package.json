{
  "name": "noir-base64-rpc-server",
  "version": "1.0.0",
  "type": "module",
  "description": "JSON RPC server for Noir base64 oracle testing",
  "main": "rpc_server.js",
  "scripts": {
    "start": "node rpc_server.js",
    "dev": "node --watch rpc_server.js",
